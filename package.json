{
  "name": "urara",
  "version": "0.0.1",
  "repository": "github:importantimport/urara",
  "scripts": {
    "clean": "node urara.js clean",
    "partytown": "partytown copylib static/~partytown",
    "tsc": "tsc -p tsconfig.node.json",
    "tsc:watch": "tsc -w -p tsconfig.node.json",
    "urara:build": "node urara.js build",
    "urara:watch": "node urara.js watch",
    "kit:dev": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite dev",
    "kit:build": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite build",
    "dev:parallel": "npm-run-all -p -r tsc:watch urara:watch \"kit:dev {@} \" --",
    "dev": "npm-run-all -s tsc partytown \"dev:parallel {@} \" --",
    "build": "npm-run-all -s partytown tsc urara:build kit:build clean",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "^1.1.4",
    "@iconify-json/heroicons-solid": "^1.1.5",
    "@iconify-json/simple-icons": "^1.1.30",
    "@sveltejs/adapter-node": "1.0.0-next.88",
    "@sveltejs/adapter-static": "1.0.0-next.41",
    "@sveltejs/adapter-vercel": "1.0.0-next.72",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.7",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "chalk": "^5.1.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "daisyui": "^2.31.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fenceparser": "^2.2.0",
    "fff-flavored-frontmatter": "^0.3.0",
    "github-slugger": "^1.4.0",
    "mdast-util-to-string": "^3.1.0",
    "mdsvex": "^0.10.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-slug": "^5.0.1",
    "remark": "^14.0.2",
    "remark-footnotes": "~2.0.0",
    "shiki-twoslash": "^3.1.0",
    "svelte": "^3.50.1",
    "svelte-bricks": "^0.1.7",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "unist-util-visit": "^4.1.1",
    "unocss": "^0.45.29",
    "vite": "^3.1.6",
    "vite-plugin-pwa": "^0.13.1",
    "workbox-window": "^6.5.4"
  },
  "type": "module",
  "dependencies": {
    "@builder.io/partytown": "^0.7.1",
    "sharp": "^0.30.7"
  }
}